# generated from proxies_cidrs.txt.erb
<%
require "base64"
require 'zlib'
require 'stringio'

if_p("ha_proxy.expect_proxy") do |cidrs|
  uncompressed = ''
  if cidrs.is_a?(Array)
    uncompressed << "\# detected cidrs provided as array in cleartext format\n"
    cidrs.each do |cidr|
      uncompressed << cidr << "\n"
    end
  end
%>
# BEGIN proxies cidrs
<%= uncompressed %>
# END proxies cidrs
<%
end
%>
